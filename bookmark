javascript:function url(){

    var date = new Date();

    var y = date.getFullYear();
    var m = date.getMonth()+1;
    var d = date.getDate();

    if(m < 10){m = '0' + m;}
    if(d < 10){d = '0' + d;}

    var date = m + "-" + d + "-" + y;
    
    date = 'create?title=' + date + ' MP&folder=1-hze9yBzK00naZvp38nQulvAtEnEF_Rp';

    return 'http://docs.google.com/document/'+ date;

} window.open(url(),"_parent");
